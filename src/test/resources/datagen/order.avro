{
        "namespace": "com.rj.gd.streams.uc1.types",
        "name": "Order",
        "type": "record",
        "fields": [
                {"name": "AccountId", "type": {
                    "type": "string",
                    "arg.properties": {
                      "regex": "ride_[1-9][0-9][0-9]"
                    }
                }},
                {"name": "Ticker", "type": {
                    "type": "string",
                    "arg.properties": {
                     "options": [ "IBM", "CSCO", "AAPL", "AMD" ]
                    }
                }},
                {"name": "Size", "type": {
                    "type": "int",
                    "arg.properties": {
                       "range": {"min": 1, "max": 10}
                    }
                }},
                {"name": "Open", "type": {
                    "type": "double",
                    "arg.properties": {
                         "range": {"min": 10.20, "max": 30.90}
                    }
                }},
                {"name": "Close", "type": {
                      "type": "double",
                      "arg.properties": {
                            "range": {"min": 10.20, "max": 30.90}
                      }
                  }}
        ]
}